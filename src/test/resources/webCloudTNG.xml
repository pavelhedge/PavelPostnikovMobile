<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Web mobile testing Suite">
    <parameter name="execution" value="cloud"/>
    <parameter name="appType" value="web"/>
    <parameter name="udid" value="DUM0219807004565"/>
    <!--parameter name="deviceName" value="Google Pixel 2 XL"></parameter-->
    <parameter name="platformName" value="Android"/>
    <parameter name="browserName" value="Chrome"/>
    <test name="Web tests">
        <groups>
            <run>
                <include name="web"/>
            </run>
        </groups>
        <classes>
            <class name="scenarios.webMobileTests"/>
        </classes>
    </test>
</suite>